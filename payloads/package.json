{
  "name": "@braintrust/payload-capture",
  "version": "0.1.0",
  "description": "Scripts to capture OpenAI and Anthropic API payloads with TypeScript type safety",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "capture": "tsx scripts/capture.ts",
    "prune": "tsx scripts/prune.ts",
    "lint": "eslint \"{cases,scripts}/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"{cases,scripts}/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "openai": "^5.22.0",
    "@anthropic-ai/sdk": "^0.63.0"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
